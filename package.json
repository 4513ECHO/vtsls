{
  "name": "vtsls",
  "description": "LSP wrapper for typescript extension of vscode",
  "version": "0.0.1-alpha.1",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "vscode": "^1.30.0"
  },
  "bin": {
    "vtsls": "dist/main.js"
  },
  "keywords": [
    "language-server-protocol"
  ],
  "dependencies": {
    "@vscode/l10n": "^0.0.10",
    "jsonc-parser": "^3.2.0",
    "semver": "5.5.1",
    "typescript": "^4.9.3",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.7",
    "vscode-uri": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@types/semver": "^5.5.0",
    "esbuild": "^0.15.14",
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:watch": "node scripts/build.js watch",
    "patch-extension": "node scripts/patch.js"
  },
  "main": "./dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yioneko/vtsls.git"
  },
  "pnpm": {
    "patchedDependencies": {
      "jsonc-parser@3.2.0": "patches/packages/jsonc-parser@3.2.0.patch"
    }
  }
}

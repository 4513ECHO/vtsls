{
  "name": "vtsls",
  "description": "LSP wrapper for typescript extension of vscode",
  "version": "0.0.5",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "vscode": "^1.30.0"
  },
  "bin": {
    "vtsls": "dist/main.js"
  },
  "keywords": [
    "language-server-protocol"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "14.x",
    "@types/semver": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "typescript": "^4.9.3",
    "eslint": "^8.28.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:watch": "node scripts/build.js watch",
    "lint": "pnpm type-check && pnpm eslint",
    "type-check": "tsc --noEmit",
    "eslint": "eslint src/**",
    "patch-extension": "node scripts/patch.js"
  },
  "main": "./dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yioneko/vtsls.git"
  }
}

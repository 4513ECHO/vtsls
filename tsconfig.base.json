{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitOverride": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "useUnknownInCatchVariables": false,
    "outDir": "./dist",
    "strictNullChecks": true,
    "strict": true,
    "types": ["node"]
  }
}
